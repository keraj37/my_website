@model string

@{
    ViewBag.Title = "AI";
}

@Html.Partial("_ProjectsPartial")

<div>
    <section id="console">
        <div class="console console-output multiline">***AI project v0.0.1***
@Model
        </div>
        <div>
            @using (Html.BeginForm("AI", "Projects", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { role = "form" }))
            { 
                <input id="console-input" type="text" name="cmd" class="console" autofocus/>
                <input id="execute" type="submit" value="Execute" class="console-button"/>
            }
        </div>
    </section>
</div>

